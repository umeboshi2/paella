<trait name="uml_common" suite="startup">
	<description>
		None
	</description>
	<parents>
		<parent>
			global
		</parent>
	</parents>
	<packages/>
	<environ>
		<trait_variable name="debrepos_incoming" trait="uml_common">
			incoming
		</trait_variable>
		<trait_variable name="hostfs_root" trait="uml_common">
			/mnt/home/username/paella
		</trait_variable>
		<trait_variable name="systarball_bkup_path" trait="uml_common">
			/mnt/home/username/roots
		</trait_variable>
	</environ>
	<templates>
		<template grp_owner="root" mode="0755" owner="root">
			root/backup_config
		</template>
		<template grp_owner="root" mode="0644" owner="root">
			root/scratchpad
		</template>
	</templates>
	<scripts/>
</trait>

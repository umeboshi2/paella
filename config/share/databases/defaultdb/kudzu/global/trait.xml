<trait name="global" suite="kudzu">
	<description/>
	<parents/>
	<packages>
		<package action="install">
			base-files
		</package>
	</packages>
	<environ>
		<adminuser>
			umeboshi
		</adminuser>
		<common_install_export>
			/freespace/shared/common
		</common_install_export>
		<common_install_msttfpath>
			various-binaries/msttfonts
		</common_install_msttfpath>
		<common_install_opencdiso>
			various-binaries/isos/TheOpenCD-v1.4.iso
		</common_install_opencdiso>
		<common_install_paelladb>
			paelladb
		</common_install_paelladb>
		<common_install_server>
			zathras.frobozz.gue
		</common_install_server>
		<domain>
			umdomain
		</domain>
		<hostname>
			umhost
		</hostname>
		<install_opencd>
			false
		</install_opencd>
		<local_address>
			10.0.0.1
		</local_address>
		<local_netmask>
			255.0.0.0
		</local_netmask>
		<local_network>
			10.0.0.0
		</local_network>
		<local_network_cidr>
			10.0.0.0/8
		</local_network_cidr>
		<mailname>
			$global_hostname
		</mailname>
		<rev_local_zone>
			10
		</rev_local_zone>
		<windowmanager>
			enlightenment
		</windowmanager>
	</environ>
	<templates>
		<template grp_owner="root" mode="0644" owner="root" package="base-files">
			root/paella/testfile
		</template>
	</templates>
	<debconfiguration/>
	<scripts/>
</trait>

<trait name="ume_testhome" suite="sarge">
	<description/>
	<parents>
		<parent>
			ume_testuser
		</parent>
	</parents>
	<packages>
		<package action="install">
			login
		</package>
	</packages>
	<environ>
		<umlume_pubkey>
			ssh-dss  umeboshi@umhost
		</umlume_pubkey>
	</environ>
	<templates>
		<template grp_owner="umeboshi" mode="0100640" owner="umeboshi" package="login">
			home/umeboshi/.ssh/authorized_keys
		</template>
		<template grp_owner="umeboshi" mode="0100600" owner="umeboshi" package="login">
			home/umeboshi/.ssh/id_dsa
		</template>
		<template grp_owner="umeboshi" mode="0100640" owner="umeboshi" package="login">
			home/umeboshi/.ssh/id_dsa.pub
		</template>
	</templates>
	<debconfiguration/>
	<scripts/>
</trait>

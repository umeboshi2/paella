	<profile name="desktest" suite="sid">
		<traits>
			<trait ord="50">
				desktop
			</trait>
			<trait ord="50">
				kde
			</trait>
			<trait ord="50">
				office
			</trait>
		</traits>
		<families>
			<family>
				75dpi
			</family>
			<family>
				frobozz
			</family>
			<family>
				insecure
			</family>
		</families>
		<environ>
			<profile_variable name="issue_release" trait="base">
				testing/desktest
			</profile_variable>
			<profile_variable name="desktop1" trait="desktop">
				main
			</profile_variable>
			<profile_variable name="desktop2" trait="desktop">
				email
			</profile_variable>
			<profile_variable name="desktop3" trait="desktop">
				chat
			</profile_variable>
			<profile_variable name="desktop4" trait="desktop">
				misc
			</profile_variable>
			<profile_variable name="desktop5" trait="desktop">
				create
			</profile_variable>
			<profile_variable name="desktop6" trait="desktop">
				music
			</profile_variable>
			<profile_variable name="gdm_bgprog" trait="desktop">
				/usr/lib/xscreensaver/rd-bomb  -root
			</profile_variable>
			<profile_variable name="gdm_bgtype" trait="desktop">
				0
			</profile_variable>
			<profile_variable name="kmods" trait="desktop">
				via82cxxx, psmouse, mousedev, hid, ohci-hcd, uhci-hcd, ehci-hcd
			</profile_variable>
			<profile_variable name="bfm_launch" trait="gkrellm">
				x-terminal-emulator  -e bash -c &quot;ps aufx | pager&quot;
			</profile_variable>
			<profile_variable name="bigfont" trait="gkrellm">
				Sans 11
			</profile_variable>
			<profile_variable name="cpu_launch" trait="gkrellm">
				x-terminal-emulator -e bash -c &quot;cat /proc/cpuinfo | pager&quot;
			</profile_variable>
			<profile_variable name="disk_launch" trait="gkrellm">
				x-terminal-emulator -e bash -c &quot;watch -n1 df -h&quot;
			</profile_variable>
			<profile_variable name="lgfont" trait="gkrellm">
				Sans 9
			</profile_variable>
			<profile_variable name="medfont" trait="gkrellm">
				Sans 10
			</profile_variable>
			<profile_variable name="mem_launch" trait="gkrellm">
				x-terminal-emulator -e bash -c &quot;watch -n1 cat /proc/meminfo&quot;
			</profile_variable>
			<profile_variable name="proc_launch" trait="gkrellm">
				x-terminal-emulator -e top
			</profile_variable>
			<profile_variable name="smfont" trait="gkrellm">
				Sans 9
			</profile_variable>
			<profile_variable name="theme" trait="gkrellm">
				Jewel_gkrellm
			</profile_variable>
			<profile_variable name="uptime_launch" trait="gkrellm">
				x-terminal-emulator
			</profile_variable>
			<profile_variable name="hostname" trait="global">
				desktest
			</profile_variable>
			<profile_variable name="widget_style" trait="kde">
				plastik
			</profile_variable>
		</environ>
	</profile>

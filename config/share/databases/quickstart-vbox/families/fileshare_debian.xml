	<family name="fileshare_debian">
		<parents/>
		<environ>
			<family_variable name="fileshare_debian_chmod" trait="fileserver_base">
				775
			</family_variable>
			<family_variable name="fileshare_debian_chown" trait="fileserver_base">
				root:staff
			</family_variable>
			<family_variable name="fileshare_debian_create" trait="fileserver_base">
				True
			</family_variable>
			<family_variable name="fileshare_debian_path" trait="fileserver_base">
				/freespace/debian
			</family_variable>
		</environ>
	</family>
